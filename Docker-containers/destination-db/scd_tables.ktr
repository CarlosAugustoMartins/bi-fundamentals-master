    <connection>dvdrental</connection>
    <sql>select row_number () over (order by s.store_id asc) + 100 store_dim_id,
	   s.store_id , 
       s2.first_name||' '||s2.last_name manager_staff_name,
       a.address store_address ,a.address2 store_address2,
	   s.last_update::date as valid_from, '9999-12-31' as valid_to, '1' as dim_active
  from store s 
  join address a 
  on s.address_id  = a.address_id 
  join staff s2 
  on s2.staff_id = s.manager_staff_id </sql>
    <limit>0</limit>
