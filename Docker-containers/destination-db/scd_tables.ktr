    <connection>dvdrental</connection>
    <sql>SELECT row_number () over (order by staff_id asc) as staff_dim_id,
       staff_id, first_name, last_name, a.address , a.address2, a.district, a.postal_code , a.phone  , 
       email, 
       case when active then 1 else 0 end as active,
       username, "password", s.last_update, picture,
       s.last_update as valid_from, '9999-12-31' as valid_to, '1' as dim_active
FROM public.staff s
join public.address a 
on s.address_id = a.address_id
;
</sql>
