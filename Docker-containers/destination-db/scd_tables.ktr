    <connection>dvdrental</connection>
    <sql>with list_actors as (
SELECT fa.film_id ,array_agg (a.first_name||' '||a.last_name) as actors
FROM public.film_actor fa
 join public.actor a 
 on fa.actor_id = a.actor_id 
group by fa.film_id 
)
SELECT row_number () over (order by f.film_id asc) + 1000 as film_dim_id,		
		f.film_id, trim(title) title, description, 
		trim(c."name") category_name,
		release_year, 
		l."name" language_name,
	   rental_duration, rental_rate, length, replacement_cost, 
	   rating, f.last_update, special_features, fulltext,
	   actors ,
       f.last_update::date as valid_from, '9999-12-31' as valid_to, '1' as dim_active
FROM public.film f
join PUBLIC.film_category fc 
 on f.film_id = fc.film_id 
join public.category c 
  on c.category_id = fc.category_id 
 join public."language" l 
  on l.language_id = f.language_id 
 join list_actors la
  on la.film_id = f.film_id ;
</sql>
